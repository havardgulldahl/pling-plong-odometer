																<xs:restriction base="xs:string">
																	<xs:enumeration value="percent"/>
																	<xs:enumeration value="actual"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
														<xs:attribute name="label" type="xs:string"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="trend">
											<xs:annotation>
												<xs:documentation>For trender som omfatter denne delen undersøkelsen</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:decimal">
														<xs:attribute name="type">
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:enumeration value="percent"/>
																	<xs:enumeration value="actual"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
														<xs:attribute name="label" type="xs:string"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="prognosis">
											<xs:annotation>
												<xs:documentation xml:lang="nb">Antagelser o.l. som kan uttrykkes ved tall</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:string">
														<xs:attribute name="type">
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:enumeration value="percent"/>
																	<xs:enumeration value="actual"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
														<xs:attribute name="label" type="xs:string"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="factual">
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:string">
														<xs:attribute name="label" type="xs:string"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
									</xs:choice>
									<xs:element name="propositions" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="proposition" type="propositionType" maxOccurs="unbounded"/>
											</xs:sequence>
											<xs:attribute name="electors" type="xs:int" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="nb">Totalt antall stemmeberettigede</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="votes" type="xs:int">
												<xs:annotation>
													<xs:documentation xml:lang="nb">Totalt antall stemmer</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="electionCoefficients" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="nb">Spesialtype for å beregne utgjevningsmandater</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="electionCoefficient" maxOccurs="unbounded">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:float">
										<xs:attribute name="proposition" type="xs:string"/>
										<xs:attribute name="county" type="xs:string"/>
										<xs:attribute name="candidateRank" type="xs:int"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="propositions" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="nb">Data for hele undersøkelsen</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="proposition" type="propositionType" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="electors" type="xs:int" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="nb">Totalt antall stemmeberettigede</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="votes" type="xs:int">
						<xs:annotation>
							<xs:documentation xml:lang="nb">Totalt antall stemmer</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="prognosis">
